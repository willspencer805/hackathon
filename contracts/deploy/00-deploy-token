module.exports = async function ({ getNamedAccounts, deployments }) {
  const { deploy, log } = deployments
  const { deployer } = await getNamedAccounts()

  log("Deploying contracts...")

  const uri = "testURI"

  await deploy("Hackathon", {
    from: deployer,
    log: true,
    args: [uri],
  })
  log("Hackathon contract deployed!")
  log("---------------------------------------------------------")
}
